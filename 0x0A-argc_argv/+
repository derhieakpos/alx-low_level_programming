#include <stdio.h>
#include <stdlib.h>

/**
 * main - Adds two numbers.
 * @argc: command line args counter.
 * @argv: stores command line args.
 *
 * Return: Zero.
 */
int main(int argc, char *argv[])
{
	int add, i, j;

	if (argc < 3 || argc > 3)
	{
		printf("Error\n");
		return (1);
	}
	else
	{
		i = atoi(argv[1]);
		j = atoi(argv[2]);
		add = i + j;
		printf("%d\n", add);
	}
	return (0);
}
